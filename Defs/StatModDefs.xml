<?xml version="1.0" encoding="UTF-8" ?>
<Defs>

    <!-- IncomingDamageFactor default=1 min=0 -->
    <ArchotechInfusions.def.StatModDef>
        <defName>IncomingDamageFactor</defName>
        <!-- Indicates color and chance to 'natural' spawn on items -->
        <TechLevel>Archotech</TechLevel>
        <!-- Thing types that can accept this stat -->
        <TypeFilter>
            <li>Apparel</li>
        </TypeFilter>

        <!-- Marks stat as 'lesser is better' -->
        <IsNegated>true</IsNegated>
        <Complexity>200</Complexity>
        <!-- (min,max,chance) - chance uses 'weight' random -->
        <Add>(-0.2, 0.2, 0.1)</Add>
        <Mul>(0.8, 1.2, 0.8)</Mul>
        <Force>(0.5, 1.2, 0.01)</Force>

    </ArchotechInfusions.def.StatModDef>

</Defs>